{
    "name": "stonecutter/laravel-log-wechat",
    "description": "Laravel Logging Channel WeChat",
    "type": "library",
    "require": {
        "php": "^7.0",
        "laravel/framework": "~5.6",
        "guzzlehttp/guzzle": "~6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "sink cup",
            "email": "sinkcup@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Stonecutter\\LaravelLogWeChat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stonecutter\\LaravelLogWeChat\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Stonecutter\\LaravelLogWeChat\\WeChatLogServiceProvider"
            ]
